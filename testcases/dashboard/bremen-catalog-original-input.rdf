    <dct:title>Innerstädtische Wanderungen nach Staatsangehörigkeit in Bremen</dct:title>
    <dcat:contactPoint rdf:nodeID="N0959a9dbd0bb4c22aa06fabd60b88add"/>
    <dct:temporal rdf:nodeID="N84fc629ba0c4417ea2caff65c714b30d"/>
