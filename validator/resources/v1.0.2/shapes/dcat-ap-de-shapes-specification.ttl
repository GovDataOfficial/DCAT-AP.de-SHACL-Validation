    sh:severity sh:Info ;
    sh:message "Optional: Sie könnten mittels dcatde:qualityProcessURI die Rechtsgrundlage für den Zugang zu den Informationen als Freitext angeben."@de .

:Dataset_Spec_qualityProcessURI_Max
    sh:name "Shape qualityProcessURI - Maximalzahl"@de ;
