:Dataset_Spec_Shape 
    a sh:NodeShape ;
    sh:targetClass dcat:Dataset ;
    sh:name "Dateset gemäß Spezifikation 1.0.2"@de ;
    sh:description "Enthält Shapes, um ein dcat-ap.de-Dateset auf seine Konformität mit der Spezifikation 1.0.2 zu überprüfen. "@de ;
    sh:deactivated false ;

    # -contentToValidate ../testcases/dataset-contributorID.ttl
    sh:property :Dataset_Spec_contributorID_Ex ; # Existenz
    sh:property :Dataset_Spec_contributorID_IRI ; # Form - IRI
    sh:property :Dataset_Spec_contributorID_List ; # in Liste

    # -contentToValidate ../testcases/dataset-qualityProcessURI.ttl
    sh:property :Dataset_Spec_qualityProcessURI_Ex ; # Existenz
    sh:property :Dataset_Spec_qualityProcessURI_Max ; # Maximalzahl
    sh:property :Dataset_Spec_qualityProcessURI_IRI ; # Form - IRI

    # -contentToValidate ../testcases/dataset-originator-maintainer-contributor-creator.ttl
    sh:property :Dataset_Spec_originator_Ex ; # Existenz
    sh:property :Dataset_Spec_originator_foafAgent ; # Form - Agent

    sh:property :Dataset_Spec_maintainer_Ex ; # Existenz
    sh:property :Dataset_Spec_maintainer_foafAgent ; # Form - Agent

    sh:property :Dataset_Spec_contributor_Ex ; # Existenz
    sh:property :Dataset_Spec_contributor_foafAgent ; # Form - Agent

    sh:property :Dataset_Spec_creator_Ex ; # Existenz
    sh:property :Dataset_Spec_creator_foafAgent ; # Form - Agent

    # -contentToValidate ../testcases/dataset-granularity.ttl
    sh:property :Dataset_Spec_granularity_Ex ; # Existenz
    sh:property :Dataset_Spec_granularity_Max ; # Maximalzahl
    sh:property :Dataset_Spec_granularity_List ; # in Liste

    # -contentToValidate ../testcases/dataset-politicalGeocodingLevelURI.ttl
    sh:property :Dataset_Spec_politicalGeocodingLevelURI_Ex ; # Existenz
    sh:property :Dataset_Spec_politicalGeocodingLevelURI_IRI ; # Form - IRI
    sh:property :Dataset_Spec_politicalGeocodingLevelURI_List ; # in Liste
    
    # -contentToValidate ../testcases/dataset-politicalGeocodingURI.ttl
    sh:property :Dataset_Spec_politicalGeocodingURI_Ex ; # Existenz
    sh:property :Dataset_Spec_politicalGeocodingURI_IRI ; # Form - IRI
    sh:property :Dataset_Spec_politicalGeocodingURI_List ; # in Listen
    
    # -contentToValidate ../testcases/dataset-geocodingDescription.ttl
    sh:property :Dataset_Spec_geocodingDescription_Ex ; # Existenz
    sh:property :Dataset_Spec_geocodingDescription_Literal ; # Form - Literal
        
    # -contentToValidate ../testcases/dataset-legalBasis.ttl
    sh:property :Dataset_Spec_legalBasis_Ex ; # Existenz
    sh:property :Dataset_Spec_legalBasis_Literal ; # Form - Literal
    .

