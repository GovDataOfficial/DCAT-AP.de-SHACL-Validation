    sh:property :Distribution_Spec_plannedAvailability_Max ;  # Maximalzahl
    sh:property :Distribution_Spec_plannedAvailability_IRI ;  # Form - IRI
    sh:property :Distribution_Spec_plannedAvailability_List ; # in Liste

    # -contentToValidate ../testcases/distribution-dct_changes.ttl
    sh:property :Distribution_Spec_dct_description_Ex ; # Existenz
    sh:property :Distribution_Spec_dct_title_Ex ;       # Existenz
    sh:property :Distribution_Spec_dct_modified_Ex ;    # Existenz
    
    # -contentToValidate ../testcases/distribution-checksum.ttl
    sh:property :Distribution_Spec_spdx_Checksum_Algo_List ; # in Liste
    .

