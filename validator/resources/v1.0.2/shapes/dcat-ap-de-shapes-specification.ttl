    sh:path dcatde:qualityProcessURI ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:message "Pflicht: Gemäß DCAT-AP.de Spezifikation 1.0.2 darf ein Dataset nicht über mehrere qualityProcessURIs verfügen."@de .

:Dataset_Spec_qualityProcessURI_IRI
    sh:name "Shape qualityProcessURI - IRI"@de ;
