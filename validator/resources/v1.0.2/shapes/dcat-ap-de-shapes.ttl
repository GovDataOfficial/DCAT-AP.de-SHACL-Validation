    sh:property :Dataset_Spec_contributorID_2 ; # Form - IRI

    # -contentToValidate tests/testcases-dataset-qualityProcessURI.ttl
    sh:property :Dataset_Spec_qualityProcessURI_1 ; # Existenz
    sh:property :Dataset_Spec_qualityProcessURI_2 ; # Maximalzahl
    sh:property :Dataset_Spec_qualityProcessURI_3 ; # Form - IRI

    # -contentToValidate tests/testcases-dataset-originator-maintainer-contributor-creator.ttl
    sh:property :Dataset_Spec_originator_1 ; # Existenz
    sh:property :Dataset_Spec_originator_2 ; # Form - Agent

    sh:property :Dataset_Spec_maintainer_1 ; # Existenz
    sh:property :Dataset_Spec_maintainer_2 ; # Form - Agent

    sh:property :Dataset_Spec_contributor_1 ; # Existenz
    sh:property :Dataset_Spec_contributor_2 ; # Form - Agent

    sh:property :Dataset_Spec_creator_1 ; # Existenz
    sh:property :Dataset_Spec_creator_2 ; # Form - Agent

    # -contentToValidate tests/testcases-dataset-granularity.ttl
    sh:property :Dataset_Spec_granularity_1 ; # Existenz
    sh:property :Dataset_Spec_granularity_2 ; # Maximalzahl
    sh:property :Dataset_Spec_granularity_3 ; # Form - IRI auf NAL

    # -contentToValidate tests/testcases-dataset-politicalGeocodingLevelURI.ttl
    sh:property :Dataset_Spec_politicalGeocodingLevelURI_1 ; # Existenz
    sh:property :Dataset_Spec_politicalGeocodingLevelURI_2 ; # Form - IRI
    sh:property :Dataset_Spec_politicalGeocodingLevelURI_3 ; # In Liste
    
    # -contentToValidate tests/testcases-dataset-politicalGeocodingURI.ttl
    sh:property :Dataset_Spec_politicalGeocodingURI_1 ; # Existenz
    sh:property :Dataset_Spec_politicalGeocodingURI_2 ; # Form - IRI
    sh:property :Dataset_Spec_politicalGeocodingURI_3 ; # In Listen
    
    # -contentToValidate tests/testcases-dataset-geocodingDescription.ttl
    sh:property :Dataset_Spec_geocodingDescription_1 ; # Existenz
    sh:property :Dataset_Spec_geocodingDescription_2 ; # Form - Literal
        
    # -contentToValidate tests/testcases-dataset-legalBasis.ttl
    sh:property :Dataset_Spec_legalBasis_1 ; # Existenz
    sh:property :Dataset_Spec_legalBasis_2 ; # Form - Literal
    
    .

